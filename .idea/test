<script type="text/javascript">
function getCookieVal(offset){
	var endstr = document.cookie.indexOf(";",offset);
	if(endstr == -1){
		endstr = document.cookie.length;
	}
	return unescape(document.cookie.substring(offset,endstr));
}
function setCookie(name, value, expiredays) //쿠키 저장함수
{
    var todayDate = new Date();
    todayDate.setTime(todayDate.getTime() + (expiredays * 86400000));
    document.cookie = name + "=" + escape(value) + "; path=/; expires="
            + todayDate.toGMTString() + ";";
}
function getCookie (name){
	var arg = name + "=";
   	var alen = arg.length;
   	var clen = document.cookie.length;
   	var i = 0;
   	while (i < clen){
   		var j = i + alen;
   		if (document.cookie.substring(i, j) == arg){
   			return getCookieVal (j);	
   		}
   		i = document.cookie.indexOf(" ", i) + 1;
   		if (i == 0){
   			break;	
   		} 
   	}
   	return null;
}
$(document).ready(function() {
	//팝업 기능 추가 (2019-05-16) - start
	if ($('.system_pop').length > 0) {
		$('.system_pop').each(function(){
			if ($(this).data('seq') != undefined && $(this).data('seq') != '') {
				if (getCookie($(this).attr('id')) != 'close') {
					if ($(this).data('type') == 'N') {
						var option = 'width='+($(this).data('width')+18)+',height='+($(this).data('height')+114)
									+',left='+ $(this).data('left')+',top='+$(this).data('top');
						window.open('/cmmn/systemPopup.mhp?seq='+$(this).data('seq'), $(this).attr('id'), option);
					} else {
						$(this).show();
					}
				}	
			}
		});
		$('.system_pop .layer_close').click(function(){
			$(this).parent().parent().hide();
		});
		$('.system_pop .btn_day').click(function(){
			var cookId = $(this).parent().parent().attr('id');
			var block = $(this).parent().parent().data('block');
			if (block == 'D'/*하루*/) {
				setCookie(cookId, "close", 1);
			} else if (block == 'W'/*일주일*/) {
				setCookie(cookId, "close", 7);
			} else /*'M'한달*/ {
				setCookie(cookId, "close", 30);
			}
		});
	}
	//팝업 기능 추가 (2019-05-16) - end
});
</script>
