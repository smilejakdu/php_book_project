<div id="container">
<section class="content" id="content">
<script>


$(document).ready(function() {	
	
	$(document).on("change","#oneDepth",function(){		
		var pMenuId = $(this).val();
		
		
		var linkUrl = $("#oneDepth option:selected").attr("data-link");		
		
		$("#twoDepth option").remove();				
		$("#twoDepth").selectBox();
		
		$("#threeDepth option").remove();
		$("#threeDepth").selectBox();
		
		$("#fourDepth option").remove();
		$("#fourDepth").selectBox();
		
		fn_location_menu(2, pMenuId, linkUrl);
		
	});
	
	$(document).on("change","#twoDepth",function(){		
		var pMenuId = $(this).val();
		var linkUrl = $("#twoDepth option:selected").attr("data-link");
		
		$("#threeDepth option").remove();
		$("#threeDepth").selectBox();
				
		$("#fourDepth option").remove();				
		$("#fourDepth").selectBox();
		
		fn_location_menu(3, pMenuId, linkUrl);
	});
	
	
	$(document).on("change","#threeDepth",function(){		
		var pMenuId = $(this).val();		
		var linkUrl = $("#threeDepth option:selected").attr("data-link");
		
		$("#fourDepth option").remove();				
		$("#fourDepth").selectBox();
		
		fn_location_menu(4, pMenuId, linkUrl);		
	});
	
	$(document).on("change","#fourDepth",function(){
		
		var linkUrl = $("#fourDepth option:selected").attr("data-link");
		
		//$("#threeDepth").parent().parent().removeClass("last_depth");
		
		$(location).attr("href",linkUrl);
				
	});
	
	//로케이션이 없을경우
	if ( $("#oneDepth option").length == 0){
		$("div.location").remove();
	}
	
	$("#twoDepth").parent().parent().hide();
	$("#threeDepth").parent().parent().hide();
	$("#fourDepth").parent().parent().hide();
	
	if( $("#twoDepth option").size() > 0){
		$("#twoDepth").parent().parent().show();	
		$("#twoDepth").selectBox();
	}
	
	if( $("#threeDepth option").size() > 0){
		$("#threeDepth").parent().parent().show();
		$("#threeDepth").selectBox();
	}
	
	if( $("#fourDepth option").size() > 0){
		$("#threeDepth").parent().parent().removeClass("last_depth");
		$("#fourDepth").parent().parent().show();
		$("#fourDepth").selectBox();
	}
	
	
});


//Location 메뉴
function fn_location_menu(menuLvl, pMenuId, linkUrl){
	
	var oneFlag = false;
	var twoFlag = false;
	var threeFlag = false;
	var fourFlag = false;
	
	var nextFlag = false;
	
	$.ajax({
		url: '/cmmn/menuList.mhpx',
		async: true,
		type: 'POST',
		data: {menuLvl : menuLvl , pMenuId : pMenuId, menuCd : 'F'},		
		success: function (data) {			
			
			// 데이터가 있을 경우
			if( data.menuList.length > 0){
				for(i=0; i < data.menuList.length; i ++){
					
					switch(data.menuList[i].menuLvl){						
						case 1	:		
							$("#oneDepth").append("<option value=\""+data.menuList[i].menuId+"\" data-link=\""+data.menuList[i].linkUrl+"\">"+data.menuList[i].menuNm+"</option>");
							oneFlag = true;
							break;
						case 2	:						
							$("#twoDepth").append("<option value=\""+data.menuList[i].menuId+"\" data-link=\""+data.menuList[i].linkUrl+"\">"+data.menuList[i].menuNm+"</option>");
							twoFlag = true;
							break;
						case 3	:						
							$("#threeDepth").append("<option value=\""+data.menuList[i].menuId+"\" data-link=\""+data.menuList[i].linkUrl+"\">"+data.menuList[i].menuNm+"</option>");
							threeFlag = true;
							break;
						case 4	:						
							$("#fourDepth").append("<option value=\""+data.menuList[i].menuId+"\" data-link=\""+data.menuList[i].linkUrl+"\">"+data.menuList[i].menuNm+"</option>");
							fourFlag = true;
							break;
						defalut	:
							break;
					}
				}
			// 데이터가 없는 경우
			}else{
				switch(menuLvl){					
					case 1	:					
						
							break;
					case 2	:
						
							$("#oneDepth").parent().parent().addClass("last_depth");						
							
							$("#twoDepth option").remove();
							$("#twoDepth").parent().parent().hide();
							
							$("#threeDepth option").remove();
							$("#threeDepth").parent().parent().hide();
							
							$("#fourDepth option").remove();
							$("#fourDepth").parent().parent().hide();		
							
							break;
					case 3	:
						
							$("#twoDepth").parent().parent().addClass("last_depth");
							
							$("#threeDepth option").remove();
							$("#threeDepth").parent().parent().hide();			
							
							$("#fourDepth option").remove();
							$("#fourDepth").parent().parent().hide();		
							
							
							break;
					case 4	:
						
							$("#threeDepth").parent().parent().addClass("last_depth");
							
							$("#fourDepth option").remove();
							$("#fourDepth").parent().parent().hide();
							
							break;
						
					defalut	:
							break;
				
				}
			}		
			
			if(oneFlag){
				$("#oneDepth").selectBox();	
			}
			if(twoFlag){
				$("#oneDepth").parent().parent().removeClass("last_depth");
				$("#twoDepth").parent().parent().show();				
				$("#twoDepth").selectBox();				
				
				$("#twoDepth").trigger("change");				
			}
			if(threeFlag){
				$("#twoDepth").parent().parent().removeClass("last_depth");
				$("#threeDepth").parent().parent().show();
				$("#threeDepth").selectBox();
				
				$("#threeDepth").trigger("change");
			}
			if(fourFlag){
				$("#threeDepth").parent().parent().removeClass("last_depth");
				$("#fourDepth").parent().parent().show();
				$("#fourDepth").selectBox();
				
				$("#fourDepth").trigger("change");
			}
			
			
			if(null != linkUrl && linkUrl != ""){
				$(location).attr("href",linkUrl);
				
			}
			return false;
		}, error: function (err) {
			alert("작업 중 오류가 발생하였습니다.");
			return false;
		}		
	});
}
</script>
