<script type="text/javascript">
	$(document).ready(function(){
		
		
		
		$("a.popup").click(function(e){
			e.preventDefault();
			window.open('/htm/popup/lawPop.htm', 'popDetailChk', 'width=820, height=900, top=100, left=100, fullscreen=no, menubar=no, status=yes, toolbar=no, titlebar=yes, location=no, scrollbars=yes, resizable=yes');
		});
		
		$("a.ppn").click(function(e){
			e.preventDefault();
			var ppnCd = $(this).data("ppncd");
			window.open('/dev/lawPpnList.mhp?ppnCd='+ppnCd, 'popDetailChk', 'width=600, height=550, top=100, left=100, fullscreen=no, menubar=no, status=yes, toolbar=no, titlebar=yes, location=no, scrollbars=yes, resizable=yes');
		});
		
		$("#doSubmit").click(function(){
			
			if ($("input[name='itemId']").val() == $("input[name='itemId']").attr("placeholder")) {
				$("input[name='itemId']").val("");
            }
			
			$("#pageIndex").val(1);
			$("#searchVO").submit();
			
		});
		
		$("select[name='orderBy']").change(function(){
			
			if ($("input[name='itemId']").val() == $("input[name='itemId']").attr("placeholder")) {
				$("input[name='itemId']").val("");
            }
			
			$("#pageIndex").val(1);
			$("#searchVO").submit();
			
		});
		
	});
	
	function fn_link_page(pageNo){
		
		if ($("input[name='itemId']").val() == $("input[name='itemId']").attr("placeholder")) {
			$("input[name='itemId']").val("");
        }
		
		$("#pageIndex").val(pageNo);
		$("#searchVO").submit();
	}
	
	
</script>
